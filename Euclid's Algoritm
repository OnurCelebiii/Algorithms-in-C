#include <stdio.h>

void swap(int *a, int *b)
{
    if(*a <* b)
    {
        int c = *a;
        *a = *b;
        *b = c;
    }
}

int EulerAlgorithm(int a,int b)
{
    if(a < b)
    {
        swap(&a,&b);
    }

    while(a / b > 0)
    {
        int c = a % b;
        
        if(a % b != 0)
        {
            int d = a;
            a = b ;
            b = d % b;
        }
        else
        {
            return b;
        }
    }
}

int main() 
{
    int a = 116298, b=461952;
    
    //scanf("%d", &a);
    //scanf("%d", &b);


    printf("a : %d \nb : %d\nEuler Result : %d",a ,b , EulerAlgorithm(a,b));
    
    
    
    return 0;
}


